<project name="corundum.target.init-jdk">

  <target name="init-jdk" depends="init-corundum">
    <for list="${jdk.versions}" param="version" delimiter=",">
      <sequential>
        <init-jdk version="@{version}"/>
      </sequential>
    </for>
  </target>

</project>